type Project {
    id: ID!
    title: String!
    projectGroup: ProjectGroup!
    notes: String
    active: Boolean!
}

input ProjectInput {
    id: ID
    title: String!
    idProjectGroup: ID!
    notes: String
    active: Boolean
}

extend type Query {
    allProject: [Project]
    project(id: ID!): Project
}

extend type Mutation {
    createProject(project: ProjectInput!): Project
    updateProject(project: ProjectInput!): Project
    deleteProject(id: ID!): Boolean
}



